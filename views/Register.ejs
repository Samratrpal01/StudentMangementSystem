    <div class="register-photo">
        <div class="form-container">
        <div class="image-holder"></div>
        <form id="myForm">
        <span id="error" class="error-message"></span>
        <h2 class="text-center"><strong>Create</strong> an account.</h2>
        <div class="form-group"><input class="form-control" type="fullname" name="name" id="fullname" placeholder="Full Name"></div>
        <div class="form-group"><input class="form-control" type="email" name="username" placeholder="Email" id="email"></div>
